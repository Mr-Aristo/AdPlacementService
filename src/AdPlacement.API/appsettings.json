{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elasticsearch:9200",
          "indexFormat": "adplacements-logs-{0:yyyy.MM.dd}",
          "autoRegisterTemplate": true,
          "batchPostingLimit": 50,
          "period": "00:00:02"
        }
      },
      { "Name": "Console" }
    ]
  }
}